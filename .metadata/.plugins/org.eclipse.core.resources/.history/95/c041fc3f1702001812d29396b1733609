package a_Zadania.b_Dzien_2.a_Napisy;

public class Main2 {

    public static void main(String[] args) {

    String str = "12321";    
    StringBuffer sb = new StringBuffer();
    sb.append(str);
    if(sb.toString().equals(sb.reverse().toString())){//StringBuffer moze zmieniac sie w trakcie uzywania komend, w przeciwienstwie do Stringa
    	System.out.println("tak");
    }
    
}
}